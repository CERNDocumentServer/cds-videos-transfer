  "title": {
    "title": "50 Years of synchrotrons",
    "subtitle": "Adams\\' Memorial lecture"
  },
  "date": "1996-11-07",
  "duration": "00:00:00",
  "physical_medium": [
    {
      "medium_standard": "Betacam",
