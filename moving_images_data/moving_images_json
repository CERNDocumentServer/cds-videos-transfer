  "title": {
    "title": "Neutrons in a highly diffusive medium",
    "subtitle": "a new propulsion tool for deep space exploration?"
  },
  "date": "1998-08-27",
  "duration": "01:15:43",
  "physical_medium": [
    {
      "medium_standard": "paper",
      "sequence_number": []
    },
    {
